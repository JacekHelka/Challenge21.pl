
/**
* Przygotuj dwie listy: pierwsza 2 elementowa typu string, a druga 3 elementowa typu int: np. {“Adam”, “Jan”} i {1, 2, 3}
* Przygotuj dwie zagnieżdżone pętle foreach (jedna w drugiej), które pozwolą pod każdym stringiem wyświetlić wszystkie wartości liczbowe z drugiej listy. Przykład:
* Adam
* 1
* 2
* 3
* Jan
* 1
* 2
* 3
*/


// See https://aka.ms/new-console-template for more information
using System;
using System.Collections.Generic;


namespace ChallengeApp
{
    class Program
    {
        static void Main(string[] args)
        {

            var imie = new List<string>{"Adam", "Jan"};
            var numbers = new List<int>{1, 2, 3};
            foreach(var i in imie)
            {
                Console.WriteLine(i);
                foreach(var n in numbers)
                {
                    Console.WriteLine(n);
                }
            }
        }
            

    }
       
}

